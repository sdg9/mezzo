{
  "version": 2,
  "projects": {
    "admin-web": "apps/admin-web",
    "admin-web-e2e": "apps/admin-web-e2e",
    "constants": "libs/constants",
    "core-client": "libs/core-client",
    "core-client-server-tests": {
      "root": "libs/core-client-server-tests",
      "sourceRoot": "libs/core-client-server-tests/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/core-client-server-tests/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-client-server-tests"],
          "options": {
            "jestConfig": "libs/core-client-server-tests/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "core-server": "libs/core-server",
    "generator-tester": {
      "root": "apps/generator-tester",
      "sourceRoot": "apps/generator-tester/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/generator-tester",
            "main": "apps/generator-tester/src/main.ts",
            "tsConfig": "apps/generator-tester/tsconfig.app.json",
            "assets": ["apps/generator-tester/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/generator-tester/src/environments/environment.ts",
                  "with": "apps/generator-tester/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "generator-tester:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/generator-tester/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/generator-tester"],
          "options": {
            "jestConfig": "apps/generator-tester/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "interceptor-react-native-with-reactotron": "libs/interceptor-react-native-with-reactotron",
    "interfaces": "libs/interfaces",
    "utils-generate-guid": {
      "root": "libs/utils-generate-guid",
      "sourceRoot": "libs/utils-generate-guid/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/utils-generate-guid/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils-generate-guid"],
          "options": {
            "jestConfig": "libs/utils-generate-guid/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils-generate-guid",
            "tsConfig": "libs/utils-generate-guid/tsconfig.lib.json",
            "packageJson": "libs/utils-generate-guid/package.json",
            "main": "libs/utils-generate-guid/src/index.ts",
            "assets": ["libs/utils-generate-guid/*.md"]
          }
        }
      },
      "tags": []
    },
    "utils-generator": {
      "root": "libs/utils-generator",
      "sourceRoot": "libs/utils-generator/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/utils-generator/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils-generator"],
          "options": {
            "jestConfig": "libs/utils-generator/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils-generator",
            "tsConfig": "libs/utils-generator/tsconfig.lib.json",
            "packageJson": "libs/utils-generator/package.json",
            "main": "libs/utils-generator/src/index.ts",
            "assets": ["libs/utils-generator/*.md"]
          }
        }
      },
      "tags": []
    },
    "utils-logger": "libs/utils-logger",
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "commitMessageFormat": "chore(${projectName}): release version ${version} (skip ci)",
            "skipProjectChangelog": true
          }
        }
      }
    }
  }
}
